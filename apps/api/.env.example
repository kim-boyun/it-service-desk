# Database (docker compose 기준: db라는 서비스명 사용)
DATABASE_URL=postgresql+psycopg2://itsd:itsd@db:5432/itsd

# JWT
JWT_SECRET=change-me
JWT_EXPIRE_MINUTES=120

# NCP Object Storage (S3 compatible)
OBJECT_STORAGE_ENDPOINT=https://kr.object.ncloudstorage.com
OBJECT_STORAGE_REGION=kr-standard
OBJECT_STORAGE_BUCKET=it-service-desk
OBJECT_STORAGE_ACCESS_KEY=change-me
OBJECT_STORAGE_SECRET_KEY=change-me

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:5173
